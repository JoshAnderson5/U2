<div class="row">
	<div class="span6 offset3">
		<div class="well">
			<%= image_tag @listing.image %>
			<p>
			  <%= @listing.description %>
			</p>
			<p>
			  <%= @listing.user.name %>
			</p>
			<% if current_user == @listing.user %>
			<%= link_to 'Edit', edit_listing_path(@pin) %> |
			<% end %>
			<%= link_to 'Back', listing_path %>
		</div>
	</div>
</div>